{
	"$schema": "http://json-schema.org/schema",
	"name": "nxink",
	"version": "0.0.1",
	"generators": {
		"init": {
			"factory": "./src/generators/init/generator",
			"schema": "./src/generators/init/schema.json",
			"description": "Initialize the nxink plugin",
			"hidden": true
		},
		"binary": {
			"factory": "./src/generators/binary/generator",
			"schema": "./src/generators/binary/schema.json",
			"description": "Create a Rust binary",
			"aliases": ["bin", "app", "application"]
		},
		"library": {
			"factory": "./src/generators/library/generator",
			"schema": "./src/generators/library/schema.json",
			"description": "Create a Rust library",
			"aliases": ["lib"]
		},
		"ink": {
			"factory": "./src/generators/ink/generator",
			"schema": "./src/generators/ink/schema.json",
			"description": "Create a ink! smart contract",
			"aliases": ["ink"]
		}
	}
}
